    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2926",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA",
      "section_id": "1798.100",
