      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2695",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA",
      "section_id": "1798.100",
      "text": "Implement reasonable security procedures and practices.",
      "type": "Obligation",
      "requirement_id": "REQ-100-5"
    },
