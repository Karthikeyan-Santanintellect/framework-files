    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2964",
      "mainLabel": "Safeguard",
      "labels": [
        "Safeguard"
      ],
      "regional_standard_regulation_id": "CPRA",
      "safeguard_id": "SG-Contractual-Safeguards",
      "description": "Contractual provisions prohibiting the retention, use, or disclosure of personal information for any purpose other than the specific business purpose defined in the contract.",
