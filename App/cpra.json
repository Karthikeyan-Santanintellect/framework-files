      "data_id": "PI",
      "name": "Personal Information",
      "sensitivity": "Standard"
    },
    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2748",
      "mainLabel": "Control",
      "labels": [
        "Control"
      ],
      "regional_standard_regulation_id": "CPRA",
      "control_id": "Ctrl-AccessControls",
