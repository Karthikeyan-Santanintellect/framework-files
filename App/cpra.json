      "section_id": "1798.185",
      "heading": "Rulemaking Authority of the CPPA"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3019",
      "mainLabel": "Section",
      "labels": [
        "Section"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.155",
      "heading": "Administrative Enforcement and Fines"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3073",
      "mainLabel": "EnforcementAction",
      "labels": [
        "EnforcementAction"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "authority": "CPPA",
      "max_penalty": "$7,500 per violation",
      "description": "Administrative fine for violations involving the personal information of consumers under 16 years of age.",
      "enforcement_id": "AdminFine-Minor"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3072",
      "mainLabel": "EnforcementAction",
      "labels": [
        "EnforcementAction"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "authority": "CPPA",
      "max_penalty": "$7,500 per violation",
      "description": "Administrative fine for intentional violations.",
      "enforcement_id": "AdminFine-Intentional"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3071",
      "mainLabel": "EnforcementAction",
      "labels": [
        "EnforcementAction"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "authority": "CPPA",
      "max_penalty": "$2,500 per violation",
      "description": "Administrative fine for unintentional violations.",
      "enforcement_id": "AdminFine-Unintentional"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3016",
      "mainLabel": "Section",
      "labels": [
        "Section"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.135",
      "heading": "Mechanisms for Opting Out of Sale or Sharing"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3036",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.135",
      "text": "Ensure that the business\u2019s privacy policy is updated at least once every 12 months.",
      "type": "Obligation",
      "requirement_id": "REQ-135-1"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3014",
      "mainLabel": "Section",
      "labels": [
        "Section"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.125",
      "heading": "The Right to Non-Discrimination for the Exercise of a Consumer's Privacy Rights"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3061",
      "mainLabel": "Right",
      "labels": [
        "Right"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.125",
      "name": "Right to Non-Discrimination for Exercising CPRA Rights",
      "right_id": "Right-to-Non-Discrimination"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3034",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.125",
      "text": "Do not discriminate against a consumer because the consumer exercised any of their rights.",
      "type": "Restriction",
      "requirement_id": "REQ-125-1"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3013",
      "mainLabel": "Section",
      "labels": [
        "Section"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.121",
      "heading": "The Right to Limit the Use and Disclosure of Sensitive Personal Information"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3060",
      "mainLabel": "Right",
      "labels": [
        "Right"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.121",
      "name": "Right to Limit Use and Disclosure of Sensitive Personal Information",
      "right_id": "Right-to-Limit-SPI"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3033",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.121",
      "text": "Stop using and disclosing sensitive personal information for purposes other than those specified in the regulations upon request.",
      "type": "Restriction",
      "requirement_id": "REQ-121-2"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3046",
      "mainLabel": "DataCategory",
      "labels": [
        "DataCategory"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "data_id": "SPI",
      "name": "Sensitive Personal Information",
      "sensitivity": "High"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3032",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.121",
      "text": "Provide a clear and conspicuous link on the business\u2019s Internet homepage(s), titled \u2019Limit the Use of My Sensitive Personal Information\u2019.",
      "type": "Obligation",
      "requirement_id": "REQ-121-1"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3069",
      "mainLabel": "EventType",
      "labels": [
        "EventType"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "name": "Global Privacy Control (GPC) Signal",
      "description": "An automated signal sent by a browser or device that communicates a consumer's choice to opt-out of the sale or sharing of their personal information.",
      "event_type_id": "OptOutSignal"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3076",
      "mainLabel": "Control",
      "labels": [
        "Control"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "control_id": "Ctrl-LimitUseLink",
      "name": "'Limit the Use of My Sensitive Personal Information' Link",
      "category": "Consent Management"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3011",
      "mainLabel": "Section",
      "labels": [
        "Section"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.115",
      "heading": "The Right to Know What Personal Information is Sold or Shared and to Whom"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3009",
      "mainLabel": "Section",
      "labels": [
        "Section"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.106",
      "heading": "Consumers' Right to Correct Inaccurate Personal Information"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3058",
      "mainLabel": "Right",
      "labels": [
        "Right"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.110",
      "name": "Right to Correct",
      "right_id": "Right-to-Correct"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3030",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.106",
      "text": "Correct inaccurate personal information upon receipt of a verifiable consumer request.",
      "type": "Obligation",
      "requirement_id": "REQ-106-1"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3068",
      "mainLabel": "EventType",
      "labels": [
        "EventType"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "name": "Verifiable Consumer Request",
      "description": "A request made by a consumer to exercise their rights that the business can reasonably verify.",
      "event_type_id": "VerifiableConsumerRequest"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3078",
      "mainLabel": "Control",
      "labels": [
        "Control"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "control_id": "Ctrl-VCR-Intake",
      "name": "Verifiable Consumer Request Intake Mechanism",
      "category": "Consumer Rights"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3008",
      "mainLabel": "Section",
      "labels": [
        "Section"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.105",
      "heading": "Consumers' Right to Delete Personal Information"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3057",
      "mainLabel": "Right",
      "labels": [
        "Right"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.105",
      "name": "Right to Delete",
      "right_id": "Right-to-Delete"
    },
    {
      "id": "4:5671a034-d722-4cbb-8511-1e384567dd8e:3029",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.105",
      "text": "Delete a consumer's personal information upon receipt of a verifiable consumer request.",
      "type": "Obligation",
      "requirement_id": "REQ-105-1"
    }
