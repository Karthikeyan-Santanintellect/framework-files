      "category": "Security"
    },
    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2731",
      "mainLabel": "Safeguard",
      "labels": [
        "Safeguard"
      ],
      "regional_standard_regulation_id": "CPRA",
      "safeguard_id": "SG-ReasonableSecurity",
      "description": "Implement reasonable security procedures and practices appropriate to the nature of the personal information to protect from unauthorized or illegal access, destruction, use, modification, or disclosure.",
      "type": "Organizational/Technical"
    },
    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2694",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA",
      "section_id": "1798.100",
      "text": "Collection, use, retention, and sharing of PI shall be reasonably necessary and proportionate.",
      "type": "Restriction",
      "requirement_id": "REQ-100-4"
    },
    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2693",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA",
      "section_id": "1798.100",
      "text": "Inform consumers of the length of time the business intends to retain each category of personal information.",
      "type": "Obligation",
      "requirement_id": "REQ-100-3"
    },
    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2692",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA",
      "section_id": "1798.100",
      "text": "Do not collect additional categories of PI or use PI for incompatible purposes without providing notice.",
      "type": "Obligation",
      "requirement_id": "REQ-100-2"
    },
    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2734",
      "mainLabel": "Safeguard",
      "labels": [
        "Safeguard"
      ],
      "regional_standard_regulation_id": "CPRA",
      "safeguard_id": "SG-Deidentification",
      "description": "Deidentify or aggregate consumer information so that it cannot reasonably identify, relate to, describe, be capable of being associated with, or be linked, directly or indirectly, to a particular consumer.",
      "type": "Technical"
    },
    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2691",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA",
      "section_id": "1798.100",
      "text": "Inform consumers at or before the point of collection about the categories of personal information to be collected.",
      "type": "Obligation",
      "requirement_id": "REQ-100-1"
    }
