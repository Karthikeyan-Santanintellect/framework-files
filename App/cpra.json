        "Control"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "control_id": "Ctrl-VCR-Intake",
      "name": "Verifiable Consumer Request Intake Mechanism",
      "category": "Consumer Rights"
    },
    {
      "id": "4:933b1b58-e496-4df8-90cf-592c911f843f:5430",
      "mainLabel": "Section",
      "labels": [
        "Section"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.105",
      "heading": "Consumers' Right to Delete Personal Information"
    },
    {
      "id": "4:933b1b58-e496-4df8-90cf-592c911f843f:5479",
      "mainLabel": "Right",
      "labels": [
        "Right"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.105",
      "name": "Right to Delete",
      "right_id": "Right-to-Delete"
    },
    {
      "id": "4:933b1b58-e496-4df8-90cf-592c911f843f:5451",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA 2.0",
      "section_id": "1798.105",
      "text": "Delete a consumer's personal information upon receipt of a verifiable consumer request.",
      "type": "Obligation",
      "requirement_id": "REQ-105-1"
    }
