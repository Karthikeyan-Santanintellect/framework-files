    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2906",
      "mainLabel": "Section",
      "labels": [
        "Section"
      ],
      "regional_standard_regulation_id": "CPRA",
      "section_id": "1798.100",
      "heading": "General Duties of Businesses that Collect Personal Information"
    },
    {
      "id": "4:d48abbdc-159e-49ec-93de-474cfd216a81:2927",
      "mainLabel": "Requirement",
      "labels": [
        "Requirement"
      ],
      "regional_standard_regulation_id": "CPRA",
      "section_id": "1798.100",
      "text": "Ensure contracts with Service Providers, Contractors, and Third Parties are in place to protect PI.",
      "type": "Obligation",
      "requirement_id": "REQ-100-6"
    },
